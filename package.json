{
  "name": "@hyas/doks-core",
  "version": "1.0.1",
  "description": "Hyas Doks core",
  "main": "index.js",
  "scripts": {
    "clean": "npm run clean:lint && npm run clean:install",
    "clean:install": "shx rm -rf package-lock.json node_modules",
    "clean:lint": "shx rm -rf .eslintcache .stylelintcache",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint --cache assets/js",
    "lint:styles": "stylelint --cache \"assets/scss/**/*.{css,sass,scss}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gethyas/doks-core.git"
  },
  "keywords": [
    "doks",
    "hyas",
    "hugo"
  ],
  "author": "Henk Verlinde",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/gethyas/doks-core/issues"
  },
  "homepage": "https://github.com/gethyas/doks-core#readme",
  "dependencies": {
    "@hyas/bootstrap": "^1.0.1",
    "@hyas/images": "^1.0.1",
    "@hyas/inline-svg": "^1.0.2",
    "@hyas/seo": "^1.0.2",
    "@tabler/icons": "^2.22.0",
    "clipboard": "^2.0.11",
    "flexsearch": "^0.7.31",
    "gethyas": "^2.0.7"
  },
  "devDependencies": {
    "auto-changelog": "^2.4.0"
  }
